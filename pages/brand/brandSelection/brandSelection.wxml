<!--pages/brandSelection/brandSelection.wxml-->
<view class="line"></view>
<view class="wrap">
  <brandlist brandsArr="{{brandsArr}}" wx:if="{{pageIsLoaded}}"></brandlist>
  <view class="loading-container">
    <view class="is-loading" wx:if="{{isLoading}}"><view class="loading-icon"></view>正在加载中</view>
    <view class="no-more" wx:if="{{loaded}}">没有更多啦~</view>
  </view>
</view>
