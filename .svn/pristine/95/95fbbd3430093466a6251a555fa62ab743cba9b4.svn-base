page{
   font-size: 26rpx;
   padding-bottom: 100rpx;
}
.form-item{
  padding:26rpx 32rpx 32rpx;
  border-bottom: 1px solid #E5E5E5;
}
.form-item.noborder{
    border-bottom: 0;
}
.form-item .label{
  color: #333333;
  font-size: 28rpx;
}
.form-item .group{
  margin-top: 16rpx;
}
.form-item .group.invoice-info{
   margin-top: 24rpx;
}
.form-item-concat{
  padding:0rpx 32rpx 32rpx;
}

.radio-btns{
  display: flex;
  font-size: 26rpx;
  margin-left: -50rpx;
}
.radio-btns>view{
  padding:12rpx;
  border: 2rpx solid #CCCCCC;
  border-radius: 6rpx;
  color: #333333;
  min-width: 116rpx;
  text-align: center;
  margin-left: 50rpx;
}
.radio-btns>view.active{
  color: #FF681D;
   border-color: #FF681D;
}
.input-text{
  /* border:1px solid #ced4da; */
  border-radius: 6rpx;
  width: 100%;
  /* padding: 6rpx 6rpx; */
  height: 24rpx;
  font-size: 26rpx;
  color: #333333;
  box-sizing: border-box;
}
.input-text.input2{
  height: 80rpx;
  background:#f5f5f5;
}
.input-text.input-phone{
 /* font-size: 28rpx; */
}

.input-text:focus,.input-text::selection{
    border-color: #FF681D;
    outline: 0;
    box-shadow: 0 0 0 0.2rem rgba(0,123,255,.25);
    font-size: 26rpx;
    color: #333333;
}

.company-form,.personal{
  /* margin-bottom: 24rpx; */
}
.company-form>.invoice-line{
    display: flex;
    align-items: center;
     /* padding: 24rpx 0;  */
    border-bottom: 1px solid #E5E5E5;
     height: 88rpx; 
    box-sizing: border-box;
}

.company-form .label{
  width: 120rpx;
}
.company-form .control{
  flex-grow: 1;
}
.invoice-tip,.invoice-tip2{
  font-size: 24rpx;
  color: #999999;
}
.invoice-tip2{
  margin-top: 28rpx;
}
.invoice-tip2>view{
  padding-bottom: 8rpx;
}
.required::before{
  content: '*';
  color: red;
}
.buttons{
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  width: 750rpx;
  box-sizing: border-box;
  /* box-shadow:0px -1px 2px 1px rgba(0,0,0,0.12); */
}
.btn{
  margin: 0;
  padding: 0;
  width: 750rpx;
  font-size: 34rpx;
  height: 98rpx;
  text-align: center;
  line-height: 98rpx;
  border-radius:0;
  border:0;
  background-color: #FF681D;
  color: #ffffff;
}
.btn::after{
  border: 0;
}
.placeholder-input,.placeholder-input2{
  color: #CCCCCC;
}
.input2{
  padding-left: 35rpx;
}