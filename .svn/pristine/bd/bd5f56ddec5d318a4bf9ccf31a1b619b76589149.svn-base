.empty-data{
  position: absolute;
  top:0;
  left: 0;
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-wrap:wrap;

}
.empty-data>view{
  text-align:center;
}
.empty-data image{
  width: 164rpx;
  height: 163rpx;
  
}
.empty-data .empty-text{
  color: #999999;
  font-size: 28rpx;
  padding-top: 30rpx;
}
