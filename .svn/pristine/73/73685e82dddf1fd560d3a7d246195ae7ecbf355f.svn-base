/* pages/goods/goodsDetail/goodsDetail.wxss */
page{
  font-size: 28rpx;
}
.container{
  padding-bottom: 200rpx;
}
.preview-wrap{
  position: relative;
}
.container.fixed .preview-wrap{
  position:fixed;
  left: 0;
  top:0;
  right: 0;
  bottom: 0;
  background-color: #000;
  z-index: 1005;
  transform-origin: 50% 50%;
  animation:previewImage ease 300ms;
  touch-action:none;
}
@keyframes previewImage{
  from{
    opacity: 0;
    transform: scale(0)
  }
  to{
     opacity: 1;
    transform: scale(1)
  }
}
.container.fixed .preview-wrap .preview-swiper{
    position:absolute;
    top:50%;
    left: 0;
    margin-top: -350rpx;
}
.preview-indicator{
  display:none;
  position:absolute;
   top:150rpx;
   width: 750rpx;
  color: #fff;
  text-align: center;
}
.container.fixed .preview-wrap .preview-indicator{
    display:block;
}

.preview-swiper{
   width:750rpx;
   height:750rpx;
  background-color: #F5F5F5;
}
.preview-item{
    display: flex;
    align-items: center;
    justify-content:center;
}
.preview-image{
   width:750rpx;
  height:750rpx;
}
.backline{
  width:750rpx;
  height:14rpx;
  position:absolute;
  bottom: 0;
  left: 0;
 background:linear-gradient(180deg,rgba(0,0,0,0),rgba(0,0,0,0.04));

}
.product-intro{
  padding: 30rpx 30rpx 20rpx;
}
.title-wrap{
  display: flex;
  width: 100%;
  position: relative;
}
.title-wrap>view:first-child{
  width: 555rpx;
  overflow: hidden;
}
.title{
  font-size: 28rpx;
  color: #333333;
  width: 554rpx;
  max-height: 80rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  display:-webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

button.share{
  margin: 0;
  padding: 0;
  border-radius: 0;
  border-top-left-radius: 39rpx;
   border-bottom-left-radius: 39rpx;
  position: absolute;
   width: 120rpx;
  height: 78rpx; 
  box-sizing: border-box;
  outline:none;
  background-color: #ffffff;
  top:50%;
  margin-top: -39rpx;
  right: -30rpx;
  /* border: solid 1rpx #efefef; */
  box-shadow: -1px 0px 8px 1px rgba(228, 216, 203, 0.3);
  font-size: 24rpx;
  color: #FF681D;
  display: flex;
  align-items: center;
  justify-content: center;
}
button.share::after {
  border: none;
}
button.share>view{
  margin-left: 5rpx;
}
.share image{
  width: 41rpx;
  height: 38rpx;
}
.subtitle{
  font-size:24rpx;
  font-family:PingFangSC-Regular;
  color:rgba(153,153,153,1);
  line-height:33rpx;
  margin-top: 16rpx;
  white-space:nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
} 
.price-wrap{
  display: flex;
  padding-top: 28rpx;
  align-items:center;

}
.sales-price{
  color: #FF681D;
  font-size: 28rpx;
}
.market-price{
  color: #999999;
  font-size: 24rpx;
  margin-left: 30rpx;
  text-decoration:line-through;
}

.service-description{
  background:rgba(250,250,250,1);
  font-size: 24rpx;
  color: #808080;
  display: flex;
  height: 70rpx;
  padding: 0rpx 30rpx;
  align-items: center;

}
.service-description>view:nth-child(n+2){
  margin-left: 36rpx;
}
.service-description text{
  margin-left: 8rpx;
}
.spec-wrap{
  color: #333333;
  padding: 24rpx 30rpx;
  height: 40rpx;
  display: flex;
  align-items: center;
  
}
.spec-wrap>view:first-child{
  margin-right: 40rpx;
}
.spec-wrap>view{
  display: inline-block;
}
.spec-list{
  color: #999999;
  flex-grow: 1;
}

/**商品详情**/
.goods-info{
  width: 750rpx;
}
.goods-info.goods-info-fixed{
  padding-top: 108rpx;
}
.goods-info.goods-info-fixed>navtab{
    position: fixed;
    top: 0;
    left: 0;
    width: 750rpx;
    z-index: 10;
}


.bottom-toolbar{
  position: fixed;
  bottom: 0;
  left: 0;
  height: 98rpx;
  width: 750rpx;
  box-sizing: border-box;
  background:rgba(250,250,250,1);
  display: flex;
  z-index: 3;

}
.toolbar-btns{
  width: 350rpx;
  box-sizing: border-box;
  height: 98rpx;
  display: flex;
  font-size: 22rpx;
  color: #808080;
}
.toolbar-btn.btn{
  padding: 0;
      padding-top: 14rpx;
    border-radius: 0;
    line-height:normal;
    font-size: 22rpx;
   color: #808080;
}
.toolbar-btn.btn::after{
  border: 0;
}
.toolbar-btn{
    flex-grow: 1;
    position: relative;
    padding-top: 14rpx;
    text-align: center;  
    border-top: 2rpx solid #E5E5E5;
    background-color: #ffffff;
}

.toolbar-btn .toolbar-btn-icon{

}
.toolbar-btn .toolbar-btn-text{
  height: 30rpx;
}
.toolbar-btn .icon-home{
  display:inline-block;
}
.toolbar-btn>view:first-child{
  position: relative;
}
.toolbar-btn .tip{
    position: absolute;
    min-width: 24rpx;
    height: 24rpx;
    border-radius: 24rpx;
    background-color: #ff681d;
    left: 70rpx;
    top: 5rpx;
    color: #FFFFFF;
    text-align: center;
    line-height: 1;
    padding: 4rpx 4rpx;
    border:solid 2rpx rgba(250,250,250,1);
}
.toolbar-btn .tip-two{
padding: 4rpx 8rpx;
}
.bottom-toolbar .bottom-btn{
  border: 0;
  box-sizing: border-box;
  border-radius: 0;
   font-size: 30rpx;
  color: #FFFFFF;
  line-height: 98rpx;
  box-shadow:none;
  text-align: center;
  height: 98rpx;
}
.bottom-toolbar .bottom-btn.shopping-cart,.bottom-toolbar .bottom-btn.buy{
  width: 200rpx;
}
.bottom-toolbar .bottom-btn.shopping-cart{
  background-color: #FFA71D;
}
.bottom-toolbar .bottom-btn.buy{
  background-color: #FF681D;
}
.bottom-toolbar .bottom-btn.disabled{
  background-color: #E5E5E5;
    width: 400rpx;
    color: #666666;
}


.separation-line{
  height:20rpx;
  background:rgba(245,245,245,1);
}

.tab-content{
   /* padding-bottom: 100rpx; */ 
}
.tab-content-title{
  font-size: 24rpx;
  color: #666666;
  display: flex;
  justify-content: center;
  padding:30rpx 0rpx; 
  align-items: center;
  height: 30rpx
}
.tab-content-title>view{
  height: 1rpx;
  background-color: #E5E5E5;
  width: 80rpx;
}
.tab-content-title>view:first-child{
  margin-right: 16rpx;
}
.tab-content-title>view:last-child{
  margin-left: 16rpx;
}
.tab-content image{
    padding: 0;
    margin: 0;
    display: block;
}
.tab-spec{
  padding: 0 30rpx;
}
.spec-table{
  border: solid 1px #e5e5e5;
  font-size: 24rpx;
  color: #666;
}
.spec-group{
  font-weight: 600;
  line-height: 70rpx;
}
.spec-group,.spec-attr{
    min-height: 70rpx;
}
.spec-group,.spec-attr>view:first-child{
   padding-left: 30rpx;
}
.spec-table .spec-item:first-child{
  border-top:0;
}
.spec-item{
    border-top: solid 2rpx #e5e5e5;
}
.spec-attr-list{
}
.spec-attr{
  display: flex;
  border-top: solid 2rpx #e5e5e5;
  align-items: center;
}

.spec-attr>view:first-child{
   min-width: 200rpx;
   width:200rpx;
}
.spec-attr>view:last-child{
  padding-left: 20rpx;
  border-left: solid 2rpx #e5e5e5;
  min-height: 70rpx;
  display: flex;
  align-items: center;
}
.spec-control{
  font-size:  26rpx;
    color:#333333;
    margin-top: 40rpx;
}
.spec-control-title{
 padding: 18rpx 0;
}
.spec-control-group{
  display: flex;
    flex-wrap:wrap;
    align-items: center;
}
.spec-control-group .radio{
  height:50rpx;
  border-radius:6rpx;
  font-size: 26rpx;
  border:1rpx solid #d9d9d9;
  display: flex;
  align-items: center;
  justify-content: center;
  text-align: center;
  color:#333333;
  padding: 0 18rpx;
  margin-right:30rpx; 
  margin-bottom: 20rpx;
}
.spec-control-group .radio.active{
    border-color: #ff681d;
    color: #ff681d;
}
.spec-control-group .radio.disabled{
    border-color: #E5E5E5;
    color: #CCCCCC;
}
.spec-dialog-intro{
  height: 90rpx;
}
.spec-dialog-intro>image{
  width:200rpx;
  height:200rpx;
  background:#F5F5F5;
  border-radius:10px;
  position: absolute;
  top:-70rpx;
}
.spec-dialog-intro>view{
  margin-left: 220rpx;
}
.spec-dialog-price{
    color: #FF681D;
  font-size: 28rpx;
}
.spec-dialog-selected{
  font-size: 26rpx;
  color: #333333;
  margin-top: 16rpx;
}
/**服务说明*/
.service-desc{
}
.service-desc-title{
  display: flex;
  align-items: center;
}
.service-desc-title>image{
  width: 40rpx;
  height: 40rpx;
}
.service-desc-title>view{
  font-size: 28rpx;
  color: #333333;
  margin-left: 12rpx;
}
.service-desc-con{
  color: #999999;
  font-size: 24rpx;
  padding-top: 12rpx;
    margin-bottom: 54rpx;
}
