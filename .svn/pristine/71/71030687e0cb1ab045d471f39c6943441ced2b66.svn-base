page{
  padding-bottom: 152rpx;
  background:#FFFFFF;
}
.head,.content-wrap{
  padding:0 32rpx;
}
.head{
  height: 88rpx;
  display: flex;
  align-items: center;
  background-color: #ffffff;
}
.head-gray{
background-color: #F5F5F5;
}
.head .label{
   color: #333333;
   font-size: 30rpx;
   font-weight: bold;
}
.head .title{
   color: #333333;
   font-size: 30rpx;
}
.content-wrap{
  font-size: 28rpx;
  color: #333333;
  padding-bottom: 40rpx;
}
.content{
  word-break: break-all;
  word-wrap:break-word;
}
.helper-line{
  height: 20rpx;
  background-color: #F5F5F5;
}
.feedback{

  font-size: 30rpx;
  color: #333333;
  padding: 60rpx 0;
}
.feedback .title{
  text-align: center;
}
.feedback-reust{
  text-align: center;
  display:flex;
  justify-content: center;
  margin-top: 33rpx;
  font-size: 28rpx;
  
}
.feedback-reust>view{
  width:540rpx;
  height:80rpx;
  background-color:rgba(245,245,245,1);
  border-radius:4rpx;
  line-height: 80rpx;
  text-align: center;
}
.feedback-reust.blue>view{

  background-color:#49A0FF;
  color: #ffffff;
}
.feedback .buttons{
  display: flex;
  justify-content:center;
  margin-top: 33rpx;
}
.feedback .buttons>.btn:nth-child(n+2){
  margin-left: 49rpx;
}
.feedback .buttons .btn{
  width:220rpx;
  height:80rpx;
  border-radius:4rpx;
  border:2rpx solid rgba(204,204,204,1);
  display: flex;
  align-items: center;
  justify-content: center;
}
.concat{
  height:109rpx;
  background:rgba(245,245,245,1);
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  color: #333333;
}
.concat>view{
   height:80rpx;
   display: flex;
    align-items: center;
  justify-content: center;
}
.helper-icon{
  width: 42rpx;
  height: 42rpx;
  background-repeat: no-repeat;
  background-position:center;
  background-size:contain;
  margin-right: 12rpx;
}
.helper-icon.icon-yes{
  background-image: url(https://devskin.manjd.net/miniprogram/images/helper/helper-yes.png);
}
.helper-icon.icon-no{
   background-image: url(https://devskin.manjd.net/miniprogram/images/helper/helper-no.png);
}
.helper-icon.icon-concat{
   width: 43rpx;
  height: 45rpx;
    margin-right: 16rpx;
  background-image: url(https://devskin.manjd.net/miniprogram/images/helper/helper-concat.png);
}